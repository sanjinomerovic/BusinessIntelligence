  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select top 1000 z.Ime +' '+z.Prezime as 'Ime prezime', r.Naziv as 'Naziv radilista', rd.OpisZaduzenja as Opis, rd.TrajanjeSati as 'Trajanje sati', rd.DatumDodjele as 'Datum dodjele'
from Zaposlenici as z join RadniNalog as rd
	on z.ZaposlenikID = rd.ZaposlenikID join Radilista as r
	on rd.RadilisteID=r.RadilisteID


	
 </CommandText>
      </Query>
