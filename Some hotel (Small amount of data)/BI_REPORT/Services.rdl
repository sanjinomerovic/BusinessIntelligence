    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText> select R.ReservationID,RS.Title as Service, E.Title as ExtraService
	 from Reservations as R join ReservationServices as RS 
	 on r.ReservationID=RS.ReservationID join ExtraFeatures as E
	 on RS.ExtraFeatureID = E.ExtraFeatureID
</CommandText>
      </Query>
