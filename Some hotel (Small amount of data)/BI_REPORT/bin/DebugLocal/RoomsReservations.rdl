  <DataSets>
    <DataSet Name="Bills">
      <Query>
        <DataSourceName>Hotel</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RoomTypePar">
            <Value>=Parameters!RoomTypePar.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select B.BillID,B.ReservationID, RO.RoomNumber,CL.FirstName,CL.LastName,B.DateIssued,B.Total 
from Reservations as R join Clients as C 
	 on R.ClientID = C.ClientID join Rooms as RM
	 on R.RoomID = RM.RoomID join ReservationServices as RS 
	 on R.ReservationID = RS.ReservationID join ExtraFeatures as EF
	 on RS.ExtraFeatureID=EF.ExtraFeatureID join Bills as B
	 on R.ReservationID=B.BillID join Rooms as RO 
	 on r.RoomID=RO.RoomID join Clients as CL 
	 on R.ClientID = CL.ClientID
where RO.RoomNumber = @RoomTypePar</CommandText>
      </Query>
